{% block hwi_oauth_content %}
    {% if error is defined and error %}
        <span>{{ error }}</span>
    {% endif %}
    {% for owner in hwi_oauth_resource_owners() %}
        <a class="social-login-btn btn btn-default" href="{{ hwi_oauth_login_url(owner) }}">Login
            with {{ owner | trans({}, 'HWIOAuthBundle') | capitalize }}</a>
    {% endfor %}
{% endblock hwi_oauth_content %}